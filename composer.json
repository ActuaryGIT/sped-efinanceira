{
    "name": "nfephp-org/sped-efinanceira",
    "type": "library",
    "description": "e-Financeira library",
    "homepage": "https://github.com/nfephp-org/sped-efinanceira",
    "keywords": ["Sped", "e-Financeira"],
    "license": ["LGPL-3.0-or-later", "GPL-3.0-or-later", "MIT"],
