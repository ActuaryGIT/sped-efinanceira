{
    "name": "nfephp-org/sped-efinanceira",
    "type": "library",
    "description": "e-Financeira library",
    "homepage": "https://github.com/nfephp/posprint",
    "keywords": ["Sped", "e-Financeira"],
    "license": ["LGPLv3+", "GPLv3+", "MIT"],
    "authors": [
        {
            "name": "Roberto L. Machado",
            "email": "linux.rlm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
	"phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
          "NFePHP\\eFinanceira\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NFePHP\\eFinanceira\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
